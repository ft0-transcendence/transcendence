{
	"name": "transcendence-backend",
	"version": "1.0.0",
	"scripts": {
		"start": "node dist/backend/main.js",
		"dev": "concurrently \"npm run dev:backend\" \"vite dev\"",
		"build": "npm run build:frontend && npm run build:backend",
		"prestart": "npm run build",
		"prebuild": "npm run db:generate",
		"dev:backend": "ts-node-dev --project backend/tsconfig.json --ignore-watch frontend --ignore-watch node_modules --respawn --transpile-only backend/main.ts",
		"dev:frontend": "vite dev --host 0.0.0.0",
		"build:frontend": "vite build",
		"build:backend": "tsc --project backend/tsconfig.json",
		"db:push": "prisma db push",
		"db:reset": "prisma migrate reset --force",
		"db:generate": "prisma generate",
		"db:studio": "prisma studio",
		"postinstall": "npm run db:generate"
	},
	"keywords": [],
	"author": "sandoramix",
	"license": "MIT",
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.8",
		"@types/passport-google-oauth20": "^2.0.16",
		"@types/ws": "^8.18.1",
		"@vercel/ncc": "^0.38.3",
		"concurrently": "^9.1.2",
		"esbuild": "^0.25.5",
		"pino-pretty": "^13.0.0",
		"prettier": "^3.5.3",
		"prisma": "^6.7.0",
		"tailwindcss": "^4.1.8",
		"ts-loader": "^9.5.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.8.3",
		"vite-node": "^3.1.4"
	},
	"dependencies": {
		"@ericedouard/fastify-socket.io": "^5.1.2",
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.0.1",
		"@fastify/flash": "^6.0.3",
		"@fastify/formbody": "^8.0.2",
		"@fastify/passport": "^3.0.2",
		"@fastify/session": "^11.1.0",
		"@fastify/static": "^8.2.0",
		"@prisma/client": "^6.7.0",
		"@trpc/client": "^11.1.4",
		"@trpc/server": "^11.1.4",
		"cookie": "^1.0.2",
		"dotenv": "^16.5.0",
		"fastify": "^5.3.2",
		"fastify-type-provider-zod": "^4.0.2",
		"flag-icons": "^7.5.0",
		"i18next": "^25.3.2",
		"npm": "^11.3.0",
		"passport-google-oauth20": "^2.0.0",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"superjson": "^2.2.2",
		"vite": "^6.3.5",
		"ws": "^8.18.3",
		"zod": "^3.24.4"
	},
	"engines": {
		"node": ">=22.16.0"
	}
}
